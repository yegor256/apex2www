{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": "google",
  "rules": {
    "comma-dangle": "off",
    "linebreak-style": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "quote-props": "off",
    "indent": [
      "error",
      2
    ],
    "max-len": [
      "error",
      {
        "code": 100
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2022
  }
}